{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/join",
        "function": "join"
      },
      {
        "source": "/leave",
        "function": "leave"
      },
      {
        "source": "/sendMessage",
        "function": "sendMessage"
      },
      {
        "source": "**",
        "destination": "index.html"
      }
    ]
  }
}
